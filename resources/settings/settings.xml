<settings>
	<setting propertyKey="@Properties.pomodoroDuration" title="@Strings.PomodoroDuration">
		<settingConfig type="numeric" min="1" />
	</setting>
	<setting propertyKey="@Properties.shortBreakDuration" title="@Strings.ShortBreakDuration">
		<settingConfig type="numeric" min="1" />
	</setting>
	<setting propertyKey="@Properties.longBreakDuration" title="@Strings.LongBreakDuration">
		<settingConfig type="numeric" min="1" />
	</setting>
	<setting propertyKey="@Properties.pomodorosBeforeLongBreak" title="@Strings.PomodorosBeforeLongBreak">
		<settingConfig type="numeric" min="1" />
	</setting>
	<setting propertyKey="@Properties.tickStrength" title="@Strings.TickStrength">
		<settingConfig type="numeric" min="0" max="100" />
	</setting>
	<setting propertyKey="@Properties.tickDuration" title="@Strings.TickDuration">
		<settingConfig type="numeric" min="80" max="700" />
	</setting>
	<setting propertyKey="@Properties.muteSounds" title="@Strings.MuteSounds">
		<settingConfig type="boolean" />
	</setting>
